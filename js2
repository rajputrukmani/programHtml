<body>
    <!--  Check no is prime ornot -->
    <script> 
    var n;
    n= parseInt(prompt("enter no"));

    for(var i=2;i<n;i++)
    {
        if(n%i===0)
    {
        break;
    }
    
    
    }

    
    if(i==n)
        {
        console.log("number is prime")
    }
    else
    {
        console.log("number is not prime")
    }
      
    
    </script>
</body>
</html>
