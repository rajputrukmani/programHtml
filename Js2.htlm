<html>
<script>

var a;
a=10;
b=2;
<!-- Program for print value using Increament AND Decreament-->
document.write("<br>5.sum of="+((++a)+(b++)));//11+2=13
document.write("<br>6.sum of="+((a++)+(++b)));//11+4=15
document.write("<br>7.sum of ="+((a++)-(++b)));//12-5=7
document.write("<br>8.sum of="+((a++)+(--b)));//13+4=17
document.write("<br>8.sum of="+((a--)+(++b)));//14+5=19
 
 </script>
</html>
